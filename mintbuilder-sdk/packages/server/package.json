    "@nomiclabs/hardhat-ethers": "^2",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "coffeescript": "^2.7.0",
    "dotenv": "^16.1.3",
    "hardhat": "^2.14.1",
    "helia": "^1.2.1",
    "mocha": "^10.2.0",
    "multiformats": "^11.0.2",
